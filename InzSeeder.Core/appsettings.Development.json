{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Seeding": {
    "Environment": "Development",
    "BatchSettings": {
      "DefaultBatchSize": 500
    },
    "Profiles": {
      "Development": {
        "EnabledSeeders": [
          "roles",
          "users",
          "user-roles",
          "system-variables"
        ],
        "StrictMode": false
      }
    }
  }
}